        <activity android:name=".TextSampleActivity" />
        <activity android:name=".RelaiveRotationSampleActivity" />
        <activity android:name=".TowRectangleSampleActivity" />
        <activity android:name=".ModiferSampleActivity" />
        <activity android:name=".EntityModiferSampleActivity" />
        <activity android:name=".ParticleSampleActivity" />
        <activity android:name=".ParticleInterfaceImplSampleActiviey" />
        <activity android:name=".ParticleSimpleSampleActivity" />
